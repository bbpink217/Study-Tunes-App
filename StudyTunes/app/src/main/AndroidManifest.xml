<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studytunes">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_tunes"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_tunes_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".ClassicalMusic" />
        <activity android:name=".NatureSounds" />
        <activity android:name=".WhiteNoise" />
        <activity android:name=".SmoothJazz" />
        <activity android:name=".LofiHipHop" />
        <activity android:name=".Opening">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>